{
  "name": "eregs-component-lib",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:components": "LIB_NAME=components vite build --emptyOutDir",
    "watch-build:components": "LIB_NAME=components vite build --watch --emptyOutDir",
    "build:main": "LIB_NAME=main vite build",
    "watch-build:main": "LIB_NAME=main vite build --watch emptyOutDir",
    "build": "npm run build:components && npm run build:main"
  },
  "dependencies": {
    "chokidar": "4.0.1",
    "lodash": "4.17.21",
    "mitt": "3.0.1",
    "shelljs": "0.10.0",
    "vue": "3.5.27",
    "vuetify": "3.11.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "6.0.3",
    "fast-glob": "3.2.12",
    "vite": "7.3.1",
    "vite-plugin-vuetify": "2.1.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.55.1"
  }
}
