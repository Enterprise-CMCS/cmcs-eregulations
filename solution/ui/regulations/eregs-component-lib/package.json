{
  "name": "eregs-component-lib",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build:components": "LIB_NAME=components vite build --emptyOutDir",
    "watch-build:components": "LIB_NAME=components vite build --watch --emptyOutDir",
    "build:main": "LIB_NAME=main vite build",
    "watch-build:main": "LIB_NAME=main vite build --watch emptyOutDir",
    "build": "npm run build:components && npm run build:main"
  },
  "dependencies": {
    "chokidar": "^3.6.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "shelljs": "^0.8.5",
    "vue": "^3.4.33",
    "vuetify": "^3.7.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "eslint-plugin-vuetify": "^2.2.0",
    "fast-glob": "^3.2.12",
    "vite": "^5.4.6",
    "vite-plugin-vuetify": "^2.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.13.0"
  }
}
